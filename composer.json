{
    "name": "parsingphase/adze-base-site",
    "description": "Example deployment wrapper for sites built on parsingphase/adze",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Richard George",
            "email": "richard@phase.org"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.phase.org"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "~5.5||^7.0",
        "components/bootstrap": "~3.2",
        "components/handlebars.js": "~2.0",
        "components/jqueryui": "~1.11",
        "parsingphase/adze": "^1.1.1",
        "parsingphase/blog": "^1.1.2",
        "parsingphase/phaseorg-styling": "^1.1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~1.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
